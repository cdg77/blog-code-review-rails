<%= render 'layouts/errors', :object => @post %>

<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :author %>
  <%= f.text_field :author %>
  <%= f.label :images %>
  <%= f.text_field :images %>
  <%= f.label :body %>
  <%= f.text_field :body %>

  <%= f.submit  %>
<% end %>
